    DataSnap.DSProviderDataModuleAdapter,
    Datasnap.DSServer, Datasnap.DSAuth;

type
  TAPIREST = class(TDSServerModule)
  private
    { Private declarations }
  public
    { Public declarations }
    function IsRunning: string;
  end;

implementation


{$R *.dfm}


uses System.StrUtils;

function TAPIREST.IsRunning;
begin
  Result := 'Running';
end;

end.

