object WSPessoa: TWSPessoa
  Height = 480
  Width = 640
  object qryPessoa: TFDQuery
    Connection = DataModule1.FDConnection1
    SQL.Strings = (
      'select *'
      'from pessoa'
      'where idpessoa = :idpessoa or coalesce(:idpessoa,0) = 0;')
    Left = 40
    Top = 24
    ParamData = <
      item
        Name = 'IDPESSOA'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
  end
  object updPessoa: TFDUpdateSQL
    Connection = DataModule1.FDConnection1
    ModifySQL.Strings = (
      'update'
      'pessoa'
      'set'
      'flnatureza = :flnatureza,'
      'dsdocumento = :dsdocumento,'
      'nmprimeiro = :nmprimeiro ,'
      'nmsegundo = :nmsegundo'
      'where'
      'idpessoa = :idpessoa;')
    Left = 160
    Top = 24
  end
end
